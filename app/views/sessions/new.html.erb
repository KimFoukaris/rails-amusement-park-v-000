<%= form_for @user, url: signin_path, method: :post do |f| %>
  <%= f.label :name %>
  <%= f.collection_select :User.all, :id, :name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "log in" %>
  Or <%= link_to(controller: 'users', action: 'new') { 'sign up' } %>
<% end %>
